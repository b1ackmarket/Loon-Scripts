#!name=WPS签到
#!desc=WPS每日签到领取会员，支持百度OCR和QwenOCR两种验证码识别方式。OCR_TYPE=1使用百度OCR(需要AK和SK)，OCR_TYPE=2使用QwenOCR(需要QWEN_COOKIE)
#!author=小白脸 & Yao Qinsher
#!homepage=https://github.com/b1ackmarket
#!icon=https://raw.githubusercontent.com/deezertidal/private/main/icons/wps.png
#!input=AK
#!input=SK
#!input=QWEN_COOKIE
#!select=OCR_TYPE,1,2
#!input=DAY
#!input=MAX_RETRIES

[Script]
http-request ^https:\/\/zt\.wps\.cn script-path=https://raw.githubusercontent.com/chavyleung/scripts/master/wps/wps.cookie.js, requires-body=true, tag=WPS获取Cookie

cron "0 8 * * *" script-path=https://raw.githubusercontent.com/b1ackmarket/Loon-Scripts/main/WPScheckin.js, tag=WPS签到, argument="AK=$AK&SK=$SK&OCR_TYPE=$OCR_TYPE&QWEN_COOKIE=$QWEN_COOKIE&DAY=$DAY&MAX_RETRIES=$MAX_RETRIES"

[MITM]
hostname = zt.wps.cn